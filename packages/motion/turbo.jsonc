{
  "$schema": "https://turbo.build/schema.json",
  "extends": ["//"],
  "tasks": {
    "build": {
      "dependsOn": [
        "//#build",
        "^build",
      ],
      "inputs": [
        "$TURBO_ROOT$/tsconfig.json",
        "./tsconfig.build.json",
        "package.json",
        "src/**",
        "rslib.config.ts",
      ],
      "outputs": [
        "dist",
      ],
    },
  },
}
