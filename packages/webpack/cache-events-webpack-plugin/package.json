  "keywords": [
    "webpack",
    "Lynx"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lynx-family/lynx-stack.git",
    "directory": "packages/webpack/cache-events-webpack-plugin"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "Yiming Li",
    "email": "yimingli.cs@gmail.com"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js"
    },
    "./package.json": "./package.json"
  },
  "types": "./lib/index.d.ts",
  "files": [
    "lib",
    "!lib/**/*.js.map",
    "CHANGELOG.md",
    "README.md"
  ],
  "scripts": {
