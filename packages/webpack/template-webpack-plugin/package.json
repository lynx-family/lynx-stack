    "object.pick": "^1.3.0"
  },
  "devDependencies": {
    "@lynx-js/test-tools": "workspace:*",
    "@lynx-js/vitest-setup": "workspace:*",
    "@microsoft/api-extractor": "catalog:",
    "@types/css-tree": "^2.3.11",
    "@types/object.groupby": "^1.0.4",
    "@types/object.pick": "^1.3.4",
    "@upupming/type-config": "0.0.1",
    "webpack": "^5.102.0"
  },
  "peerDependencies": {
    "@upupming/type-config": ">=0.0.1"
  },
  "engines": {
    "node": ">=18"
  }
