{
  "name": "@lynx-js/tailwind-preset",
  "version": "0.0.4",
  "description": "A tailwindcss preset for ReactLynx",
  "keywords": [
    "Lynx",
    "ReactLynx",
    "tailwindcss"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lynx-family/lynx-stack.git",
    "directory": "packages/third-party/tailwind-preset"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./lib/lynx.d.ts",
      "import": "./lib/lynx.js",
      "require": "./lib/lynx.cjs",
      "default": "./lib/lynx.js"
    }
  },
  "main": "./lib/lynx.js",
  "types": "./lib/lynx.d.ts",
  "files": [
    "lib",
    "!lib/__tests__",
    "!lib/**/*.js.map",
    "CHANGELOG.md",
    "README.md"
  ],
  "scripts": {
    "build": "rslib build",
    "test": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@lynx-js/types": "3.3.0",
    "@rslib/core": "^0.9.2",
    "tailwindcss": "^3.4.17"
  }
}
