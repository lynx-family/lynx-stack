{
  "name": "@lynx-js/tailwind-preset",
  "version": "0.0.4",
  "description": "A tailwindcss preset for ReactLynx",
  "keywords": [
    "Lynx",
    "ReactLynx",
    "tailwindcss"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lynx-family/lynx-stack.git",
    "directory": "packages/third-party/tailwind-preset"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/lynx.d.ts",
      "import": "./dist/lynx.js",
      "require": "./dist/lynx.cjs",
      "default": "./dist/lynx.js"
    }
  },
  "main": "./dist/lynx.js",
  "types": "./dist/lynx.d.ts",
  "files": [
    "dist",
    "!dist/__tests__",
    "!dist/**/*.js.map",
    "CHANGELOG.md",
    "README.md"
  ],
  "scripts": {
    "build": "rslib build",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "tsc": "tsc -b tsconfig.build.json"
  },
  "devDependencies": {
    "@lynx-js/types": "3.3.0",
    "tailwindcss": "^3.4.17"
  }
}
