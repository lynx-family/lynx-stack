{
  "name": "@lynx-js/motion-lynx",
  "version": "0.0.1",
  "private": false,
  "description": "This is a motion adapter for Lynx.js",
  "keywords": [
    "react",
    "lynx",
    "motion",
    "framer-motion"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lynx-family/lynx-stack.git",
    "directory": "packages/third-party/motion"
  },
  "license": "MIT",
  "sideEffects": [
    "./src/polyfill/shim.ts",
    "./dist/polyfill/shim.js"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./mini": {
      "types": "./dist/mini/index.d.ts",
      "import": "./dist/mini/index.js",
      "require": "./dist/mini/index.js",
      "default": "./dist/mini/index.js"
    }
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "test": "vitest run"
  },
  "dependencies": {
    "framer-motion": "12.23.12",
    "motion-dom": "12.23.12",
    "motion-utils": "12.23.6"
  },
  "devDependencies": {
    "@lynx-js/react": "workspace:*",
    "@lynx-js/types": "3.4.11"
  },
  "peerDependencies": {
    "@lynx-js/react": "*",
    "@lynx-js/types": "*"
  },
  "peerDependenciesMeta": {
    "@lynx-js/types": {
      "optional": true
    }
  }
}
